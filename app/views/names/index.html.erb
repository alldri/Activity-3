<p id="notice"><%= notice %></p>

<h1>Names</h1>

<table>
  <thead>
    <tr>
      <th>String</th>
      <th>Seat id seq</th>
      <th>String</th>
      <th>Address</th>
      <th>String</th>
      <th>Price</th>
      <th>Float</th>
      <th>Email address</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name.string %></td>
        <td><%= name.seat_id_seq %></td>
        <td><%= name.string %></td>
        <td><%= name.address %></td>
        <td><%= name.string %></td>
        <td><%= name.price %></td>
        <td><%= name.float %></td>
        <td><%= name.email_address %></td>
        <td><%= name.string %></td>
        <td><%= link_to 'Show', name %></td>
        <td><%= link_to 'Edit', edit_name_path(name) %></td>
        <td><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Name', new_name_path %>
